{
  "name": "@zoralabs/zdk",
  "description": "An SDK for using the Zora Media Protocol",
  "version": "0.2.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Zora",
  "license": "UNLICENSED",
  "private": false,
  "scripts": {
    "make-badges": "istanbul-badges-readme --coverageDir=./coverage",
    "test": "jest",
    "clean": "trash dist",
    "build": "yarn clean && tsc",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@zoralabs/core": "^1.0.2",
    "@zoralabs/media-metadata-schemas": "^0.0.6",
    "sjcl": "^1.0.8",
    "tiny-invariant": "^1.1.0",
    "tiny-warning": "^1.0.3",
    "ts-node": "^9.1.1"
  },
  "peerDependencies": {
    "@ethersproject/abstract-signer": "^5.0.5",
    "@ethersproject/address": "^5.0.5",
    "@ethersproject/bignumber": "^5.0.5",
    "@ethersproject/bytes": "^5.0.5",
    "@ethersproject/contracts": "^5.0.5",
    "@ethersproject/networks": "^5.0.5",
    "@ethersproject/providers": "^5.0.5",
    "@ethersproject/solidity": "^5.0.5",
    "@ethersproject/transactions": "^5.0.5"
  },
  "devDependencies": {
    "@ethersproject/abstract-signer": "^5.0.5",
    "@ethersproject/address": "^5.0.5",
    "@ethersproject/bignumber": "^5.0.5",
    "@ethersproject/bytes": "^5.0.5",
    "@ethersproject/contracts": "^5.0.5",
    "@ethersproject/networks": "^5.0.5",
    "@ethersproject/providers": "^5.0.5",
    "@ethersproject/solidity": "^5.0.5",
    "@ethersproject/transactions": "^5.0.5",
    "@types/jest": "^26.0.20",
    "@types/sjcl": "^1.0.29",
    "ethers": "^5.0.24",
    "husky": "^4.3.6",
    "istanbul-badges-readme": "^1.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "trash": "^7.0.0",
    "trash-cli": "^4.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn run test && yarn run make-badges && git add README.md"
    }
  },
  "lint-staged": {
    "*.{ts,md}": "prettier --write"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ourzora/zora-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ourzora/zora-sdk/issues"
  },
  "homepage": "https://github.com/ourzora/zora-sdk#readme"
}
